  },
  "keywords": [
    "emot",
    "emoji",
    "emoticons"
  ],
  "author": "Lete乐特",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lete114/emot.git"
  },
  "bugs": {
    "url": "https://github.com/Lete114/emot/issues"
  },
  "homepage": "https://github.com/Lete114/emot#readme",
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@rollup/plugin-babel": "^5.3.1",
    "eslint": "^8.16.0",
    "prettier": "^2.6.2",
    "rollup": "^2.3.4",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.0"
  }
